<h1>Sign Up</h1>

<%= form_for(@customer) do |f| %>

    <%= f.label :username %>
    <%= f.textfield :username %>

    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password :password %>

    <%= f.label :name %>
    <%= f.textfield :name %>

    <%= f.submit "Submit" %>

<% end %>